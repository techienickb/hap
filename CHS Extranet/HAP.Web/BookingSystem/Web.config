<?xml version="1.0"?>
<configuration>
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
      <identity impersonate="false" />
    </system.web>
  <location path="Default.aspx">
    <system.web>
      <authorization>
        <allow roles="Domain Admins, CHS Teaching Staff, CHS Non-Teaching Staff"/>
        <deny users="*"/>
      </authorization>
    </system.web>
  </location>
</configuration>
